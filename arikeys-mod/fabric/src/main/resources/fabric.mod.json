  "contact": {
      "homepage": "https://github.com/NerfOscar/AriKeys-Redux/wiki",
      "issues": "https://github.com/NerfOscar/AriKeys-Redux/issues",
      "sources": "https://github.com/NerfOscar/AriKeys-Redux"
  },
  "authors": ["ASangarin", "NerfOscar"],
  "license": "MIT",
  "icon": "assets/arikeysredux/icon.png",
  "environment": "client",
  "entrypoints": {
    "client": [
      "org.nerfoscar.arikeysredux.fabric.AriKeysFabric"
    ]
  },
  "mixins": [
    "arikeys.mixins.json",
    "arikeys-fabric.mixins.json"
  ],
  "depends": {
    "fabric": "*",
    "minecraft": ">=1.20.4"
  }
